<!DOCTYPE html>
<html>

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <title>Loading...</title>
  <link rel="shortcut icon" href="" />
  <!-- Hot reload client page -->
  <script src="/reload/reload.js"></script>


  <!-- Bootstrap -->
  <link rel="stylesheet" href="/bootstrap/bootstrap.min.css" />
  <!-- Datatable -->
  <link rel="stylesheet" href="/datatable/dataTables.bootstrap4.min.css" />
  <!-- Fontawesome -->
  <link rel="stylesheet" href="/fontawesome/css/all.css" />
  <!-- Select2 -->
  <link rel="stylesheet" href="/select2/select2.min.css">
  <link rel="stylesheet" href="/select2/select2-bootstrap4.min.css">
  <!-- DateRangePicker -->
  <link rel="stylesheet" href="/daterangepicker/daterangepicker.css">
  <!-- Bootstrap Template -->
  <link href="/stylesheets/sb-admin-2.css" rel="stylesheet" />
  <!--  -->
  <link rel="stylesheet" href="/stylesheets/style.css" />
</head>

<body id="page-top">
  <%include globalLoading %>
  <!-- Page Wrapper -->
  <div id="wrapper">
    <!-- Sidebar -->
    <% include sidebar%>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
      <!-- Main Content -->
      <div id="content">
        <!-- Topbar -->
        <% include topbar%>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid mt-3">
          <!-- DataTales -->
          <div class="col-xl-12 col-md-12 mb-4">
            <div class="card border-left-primary shadow h-100 py-2">
              <div class="card-body">
                <div class="row no-gutters align-items-center">
                  <div class="col">
                    <div class="text-xs font-weight-bold text-primary text-uppercase mb-3">
                      Thông tin cáp nhánh
                    </div>
                    <div class="h6 text-dark font-weight-bold d-flex flex-row justify-content-start">
                      <div class="mr-3">
                        <i class="fas fa-circle"></i>
                        Tên tuyến cáp nhánh: <span class="name"></span>
                      </div>
                      <div class="mr-3">
                        <i class="fas fa-circle"></i>
                        Điểm đầu: <span class="start"></span>
                      </div>
                      <div class="mr-3">
                        <i class="fas fa-circle"></i>
                        Điểm cuối: <span class="end"></span>
                      </div>
                    </div>
                    <div class="h6 text-dark font-weight-bold d-flex flex-row justify-content-start">
                      <div class="mr-3">
                        <i class="fas fa-circle"></i>
                        Chiều dài tuyến: <span class="length"></span>
                      </div>
                      <div class="mr-3" id="maxCore">
                        <i class="fas fa-circle"></i>
                        Core/Used: <span class="core"></span>
                      </div>
                      <div class="mr-3" id="maxCore">
                        <i class="fas fa-circle"></i>
                        SL MX: <span class="mx"></span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-xl-12 col-md-12 mb-4">
            <div class="card border-left-primary shadow h-100 py-2">
              <div class="card-body">
                <div class="row no-gutters align-items-center">
                  <div class="col">
                    <div class="card shadow mb-4">
                      <div class="card-header py-3">
                        <h6 class="m-0 font-weight-bold text-primary">
                          <i class="fas fa-tools mr-2"></i>Quản lý thiết bị
                        </h6>
                      </div>
                      <div class="card-body">
                        <nav>
                          <div class="nav nav-tabs" id="nav-tab" role="tablist">
                            <a class="nav-item nav-link active font-weight-bold" id="nav-core-tab" data-toggle="tab"
                              href="#nav-core" role="tab" aria-controls="nav-core" aria-selected="true">Core</a>
                            <a class="nav-item nav-link font-weight-bold" id="nav-mx-tab" data-toggle="tab"
                              href="#nav-mx" role="tab" aria-controls="nav-mx" aria-selected="false">MX</a>
                          </div>
                        </nav>
                        <div class="tab-content" id="nav-tabContent">
                          <div class="tab-pane fade show active" id="nav-core" role="tabpanel"
                            aria-labelledby="nav-olt-tab">
                            <div class="table-responsive mt-3">
                              <!-- Button -->
                              <a href="#" class="btn btn-primary btn-icon-split btn-lg mb-3" id="addCoreButton">
                                <span class="icon text-white-50">
                                  <i class="fas fa-plus-circle"></i>
                                </span>
                                <span class="text font-weight-bold">Thêm mới</span>
                              </a>
                              <a href="#" class="btn btn-danger btn-icon-split btn-lg mb-3" id="addCoresButton">
                                <span class="icon text-white-50">
                                  <i class="fas fa-plus-circle"></i>
                                </span>
                                <span class="text font-weight-bold">Thêm nhiều</span>
                              </a>
                              <table id="core_table" class="table table-striped table-bordered" style="width: 100%;">
                                <thead class="thead-dark">
                                  <tr>
                                    <th>STT</th>
                                    <th>Id</th>
                                    <th>Code</th>
                                    <th>Tên core</th>
                                    <th>Port nguồn</th>
                                    <th>Port đích</th>
                                    <th>Tên thiết bị nguồn</th>
                                    <th>Port thiết bị nguồn</th>
                                    <th>Tên thiết bị đích</th>
                                    <th>Port thiết bị đích</th>
                                    <th>Trạng thái</th>
                                    <th>Mục đích</th>
                                    <th>Ghi chú</th>
                                    <th>Edit</th>
                                    <th>Delete</th>
                                  </tr>
                                </thead>
                                <tbody></tbody>
                              </table>
                            </div>
                          </div>
                          <div class="tab-pane fade" id="nav-mx" role="tabpanel" aria-labelledby="nav-mx-tab">
                            <div class="table-responsive mt-3">
                              <table id="mx_table" class="table table-striped table-bordered" style="width:100%">
                                <thead class="thead-dark">
                                  <tr>
                                    <th>STT</th>
                                    <th>Id</th>
                                    <th>Code</th>
                                    <th>Ký hiệu</th>
                                    <th>Order</th>
                                    <th>Chiều dài</th>
                                    <th>Ngày lắp đặt</th>
                                    <th>POP bắt đầu</th>
                                    <th>Delete</th>
                                  </tr>
                                </thead>
                                <tbody></tbody>
                              </table>
                            </div>
                          </div>
                        </div>
                      </div>

                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- /.container-fluid -->
      </div>
      <!-- End of Main Content -->
    </div>
    <!-- End of Content Wrapper -->

    <% include coreTNModal %>
    <% include coresTNModal %>
    <%include inforModal %>
    <% include deleteModal%>
    <% include toast%>
  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Jquery -->
  <script src="/jquery/jquery-3.4.1.js"></script>
  <script src="/jquery/jquery.easing.js"></script>
  <script src="/jquery/jquery.validate.js"></script>
  <!-- Bootstrap -->
  <script src="/bootstrap/popper.min.js"></script>
  <script src="/bootstrap/bootstrap.min.js"></script>
  <!-- Bootstrap Template -->
  <script src="/javascripts/sb-admin-2.js"></script>
  <!-- Datatable -->
  <script src="/datatable/jquery.dataTables.min.js"></script>
  <script src="/datatable/dataTables.bootstrap4.min.js"></script>
  <script src="/datatable/ellipsis.js"></script>
  <!-- Select2 -->
  <script src="/select2/select2.full.min.js"></script>
  <!-- DateRangePicker -->
  <script src="/daterangepicker/moment.min.js"></script>
  <script src="/daterangepicker/daterangepicker.js"></script>
  <!-- Personal JS -->
  <script src="/javascripts/common.js"></script>
  <script src="/javascripts/map/clear.js"></script>
  <script src="/javascripts/cable/managed-cables-tn.js"></script>
</body>

</html>